database mcf_NTuBld 0100
incname mcf_NTuBld
#
# dbin template files for NTuBuild : save the data description language as 
# a dbin file.  P. Lebrun, October 1995.
# First, a generic line, as we can keep the entire comment text in a single 
# line 
template line_title
  char line " " 
end template
#
template header
    char title 
    char version
    char nameMaxIndex
    int maxMult
    int orgStyle
    int nVar
end template
#
template variable
    char name 
    char description 
    int  type   
    char isFixedSize
    int  numDim 
    int  dimensions(5)
end template 
end
