import model loop_sm-no_b_mass

define p = p b b~
define j = j b b~

define ell+ = e+ mu+ ta+
define ell- = e- mu- ta- 

generate p p > t t~ ell+ vl [QCD] @0
add process p p > t t~ ell- vl~ [QCD] @1
add process p p > t t~ ell+ vl j [QCD] @2
add process p p > t t~ ell- vl~ j [QCD] @3

output TTWJetsToLNu_5f_NLO_FXFX -nojpeg

