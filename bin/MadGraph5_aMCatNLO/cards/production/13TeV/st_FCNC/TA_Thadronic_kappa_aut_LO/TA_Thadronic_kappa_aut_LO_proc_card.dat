# Model
import model TopFCNC_UFO-kappa_aut_ProdConventions_no_c_mass --modelname

# Multiparticles
define w   = w+ w-
define l+  = e+ mu+ ta+
define l-  = e- mu- ta-
define vl  = ve vm vt
define vl~ = ve~ vm~ vt~
define j   = g u u~ d d~ c c~ s s~ b b~
define p   = g u u~ d d~ c c~ s s~

# Processes
generate    p p > t  a, (t  > w+ b,  w+ > j  j  ) NP=1 @10
add process p p > t~ a, (t~ > w- b~, w- > j  j  ) NP=1 @20

# Output folder
output TA_Thadronic_kappa_aut_LO -f -nojpeg
