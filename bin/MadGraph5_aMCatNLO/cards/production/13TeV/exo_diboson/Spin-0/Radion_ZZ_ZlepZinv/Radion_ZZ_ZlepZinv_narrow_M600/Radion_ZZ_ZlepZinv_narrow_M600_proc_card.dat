set group_subprocesses Auto
set ignore_six_quark_processes False
set gauge unitary
set complex_mass_scheme False
import model heft_radion
define p = g u c d s u~ c~ d~ s~
define l+ = e+ mu+ ta+
define l- = e- mu- ta-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~
generate p p > h2, ( h2 > z z > l+ l- vl vl~)
output Radion_ZZ_ZlepZinv_narrow_M600 -nojpeg

