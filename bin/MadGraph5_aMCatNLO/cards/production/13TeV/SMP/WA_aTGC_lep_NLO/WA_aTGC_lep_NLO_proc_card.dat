import model loop_sm-no_b_mass
import model EWdim6NLO

define p = p b b~
define j = j b b~

define ell+ = e+ mu+
define ell- = e- mu-
define V = w+ w-
define vl = ve vm
define vl~ = ve~ vm~

generate p p > ell+ vl a [QCD] @0
add process p p > ell- vl~ a [QCD] @1
add process p p > ell+ vl a j [QCD] @2
add process p p > ell- vl~ a j [QCD] @3

output WA_aTGC_lep_NLO -nojpeg


