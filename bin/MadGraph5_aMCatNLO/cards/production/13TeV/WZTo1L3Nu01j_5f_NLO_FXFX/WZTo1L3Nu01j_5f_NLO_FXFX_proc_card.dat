import model loop_sm-ckm_no_b_mass
#switch to diagonal ckm matrix if needed for speed
#import model loop_sm-no_b_mass

#include b quark in proton and jet definition for consistent 5 flavour scheme treatment
define p = p b b~
define j = j b b~

define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-

generate p p > ell- vl~ vl vl~ [QCD] @0
add process p p > ell- vl~ vl vl~ j [QCD] @1
add process p p > ell+ vl vl vl~ [QCD] @2
add process p p > ell+ vl vl vl~ j [QCD] @3

output WZTo1L3Nu01j_5f_NLO_FXFX -nojpeg

