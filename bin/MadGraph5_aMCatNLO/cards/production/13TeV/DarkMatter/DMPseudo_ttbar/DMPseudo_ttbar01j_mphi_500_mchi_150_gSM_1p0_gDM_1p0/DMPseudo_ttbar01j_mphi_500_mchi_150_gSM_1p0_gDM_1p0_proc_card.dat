define j = g u c d s u~ c~ d~ s~
define l+ = e+ mu+
define l- = e- mu-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~
# Specify process(es) to run
#generate p p > t t~ chi chi~

generate p p > t t~ chi chi~ @0
add process  p p > t t~ chi chi~ j @1
