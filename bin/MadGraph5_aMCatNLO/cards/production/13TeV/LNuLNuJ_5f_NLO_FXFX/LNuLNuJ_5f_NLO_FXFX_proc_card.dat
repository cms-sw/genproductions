define vl = ve vm vt
define vl~ = ve~ vm~ vt~

generate p p > ell+ vl ell- vl~ [QCD] @0
add process p p > ell+ vl- ell- vl~ j [QCD] @1

output lnulnuj_5f_NLO_FXFX -nojpeg

