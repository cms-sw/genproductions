set group_subprocesses Auto
set ignore_six_quark_processes False
set loop_optimized_output True
set complex_mass_scheme False
import model sm-ckm_no_b_mass
define p = g u c d s b u~ c~ d~ s~ b~
define j = p
define l+ = e+ mu+ ta+
define l- = e- mu- ta-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~

generate    p p > t w- j  QED=3 QCD=0, (t > w+ b, w+ > l+ vl) @ 1
add process p p > t~ w+ j QED=3 QCD=0, (t~ > w- b~, w- > l- vl~) @ 2
output TWj_ewk