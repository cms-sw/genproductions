#import model loop_sm-no_b_mass

#include b quark in proton and jet definition for consistent 5 flavour scheme treatment
define p = p b b~
define j = j b b~

define ell+ = e+ mu+ ta+
define ell- = e- mu- ta-

generate p p > ell- vl~ ell+ vl $$ t t~ H [QCD] @0
add process p p > ell- vl~ ell+ vl j $$ t t~ H [QCD] @1

output VVTo2L2Nu01j_5f_NLO_FXFX -nojpeg
