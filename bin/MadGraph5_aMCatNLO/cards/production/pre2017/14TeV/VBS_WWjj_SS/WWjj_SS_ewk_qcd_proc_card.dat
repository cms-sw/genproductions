set group_subprocesses Auto
set ignore_six_quark_processes False
set loop_optimized_output True
set complex_mass_scheme False

import model sm-ckm_no_b_mass

define l+ = e+ mu+ ta+
define l- = e- mu- ta-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~

generate p p > w+ w+ j j QED=4 QCD=99, w+ > l+ vl @ 1
add process p p > w- w- j j QED=4 QCD=99, w- > l- vl~ @ 2
output WWjj_SS_ewk_qcd
