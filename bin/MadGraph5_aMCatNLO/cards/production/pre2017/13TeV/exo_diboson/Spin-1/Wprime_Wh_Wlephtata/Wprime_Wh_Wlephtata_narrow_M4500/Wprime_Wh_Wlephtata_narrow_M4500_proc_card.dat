set group_subprocesses Auto
set ignore_six_quark_processes False
set gauge unitary
set complex_mass_scheme False
import model Vector_Triplet_free_Width_UFO
define l+ = e+ mu+ ta+
define l- = e- mu- ta-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~
define vc = vc+ vc-
define vw = w+ w-
define lep = l+ l-
define neu = vl vl~
generate p p > vc, (vc > h vw, h > ta+ ta-, vw > lep neu)
output Wprime_Wh_Wlephtata_narrow_M4500 -nojpeg

