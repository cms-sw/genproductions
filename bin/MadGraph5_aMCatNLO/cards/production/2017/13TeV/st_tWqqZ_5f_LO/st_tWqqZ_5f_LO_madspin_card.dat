#Some options (uncomment to apply)
set ms_dir ./madspingrid
# set Nevents_for_max_weigth 250
# set BW_cut 15
set max_weight_ps_point 400
set max_running_process 1

define l+ = e+ mu+ ta+
define l- = e- mu- ta-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~
define q = u c d s b
define q~ = u~ c~ d~ s~ b~

decay t > w+ b, w+ > l+ vl
decay t~ > w- b~, w- > l- vl~
decay w+ > q q~
decay w- > q q~
decay z > all all
# running the actual code
launch
