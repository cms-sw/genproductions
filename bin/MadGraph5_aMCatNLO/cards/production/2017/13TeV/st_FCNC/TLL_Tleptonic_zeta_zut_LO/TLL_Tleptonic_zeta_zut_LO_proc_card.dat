# Model
import model TopFCNC_UFO-zeta_zut_ProdConventions_no_c_mass --modelname
# Multiparticles
define l+  = e+ mu+ ta+
define l-  = e- mu- ta-
 
generate    p p > t  z, z > l+ l-, (t  > w+ b,  w+ > l+ vl ) NP=1 @10
add process p p > t~ z, z > l+ l-, (t~ > w- b~, w- > l- vl~) NP=1 @20

# Output folder
output TLL_Tleptonic_zeta_zut_LO -f -nojpeg
