set group_subprocesses Auto
set ignore_six_quark_processes False
set loop_optimized_output True
set loop_color_flows False
set gauge unitary
set complex_mass_scheme False
import model sm
define p = g u c d s u~ c~ d~ s~
define j = g u c d s u~ c~ d~ s~
define l+ = e+ mu+
define l- = e- mu-
define vl = ve vm vt
define vl~ = ve~ vm~ vt~
import model HC_NLO_X0_UFO-no_b_mass
define p = p b b~
define j = j b b~
define tt = t t~
define ww = w+ w-
generate p p > x0 t w-, (t > w+ b, w+ > all all )
add process p p > x0 t~ w+, (t~ > w- b~, w- > all all)
output thw_5f_ckm_LO -nojpeg
